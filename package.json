  "dependencies": {
    "@actions/artifact": "^0.5.1",
    "@actions/core": "^1.2.7",
    "@actions/github": "^4.0.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-ban": "^1.5.2",
    "eslint-plugin-license-header": "^0.2.0",
